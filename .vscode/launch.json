{
	"version": "0.2.0",
	"configurations": [
		
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/app.ts",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "internalConsole",
			"sourceMaps": true,
			//"outDir": "${workspaceRoot}/js",
			"outFiles": [
				"${workspaceRoot}/js/*.js",
				"${workspaceRoot}/js/**/*.js"
			]//,
			//"trace": "sm"
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}